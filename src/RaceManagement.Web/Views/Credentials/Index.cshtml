@model List<RaceManagement.Shared.DTOs.CredentialDto>

<h2>Danh sách Credential</h2>
<p><a class="btn btn-primary" asp-action="Create">+ Thêm Credential</a></p>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Tên</th>
            <th>Email Service Account</th>
            <th>File</th>
            <th>Trạng thái</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var c in Model)
        {
            <tr>
                <td>@c.Id</td>
                <td>@c.Name</td>
                <td>@c.ServiceAccountEmail</td>
                <td>@c.FileName</td>
                <td>@c.Status</td>
            </tr>
        }
    </tbody>
</table>
